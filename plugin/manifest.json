{
	"id": "typescript.webpack.wakatime",
	"name": "Wakatime",
	"version": "1.0.0",
	"main": "index.html",
	"host": {
		"app": ["PPRO", "PHXS"],
		"minVersion": "23.0.0",
		"data": {
			"loadEvent": "startup"
		}
	},
	"manifestVersion": 5,
	"featureFlags": {
		"enableSWCSupport": true
	},
	"requiredPermissions": {
		"network": {
			"domains": ["https://*.wakatime.com", "https://api.github.com"]
		},
		"launchProcess": {
			"schemes": ["https"]
		},
		"localFileSystem": "readwrite",
		"webview": {
			"allow": true
		},
		"clipboard": "readwrite",
		"inputEvents": true
	},
	"extensions": {
		"cep": {
			"events": {
				"all": true
			},
			"persistent": true,
			"autoVisible": true,
			"parameters": "--enable-nodejs --mixed-context"
		}
	},
	"entrypoints": [
		{
			"type": "panel",
			"id": "wakatime",
			"label": {
				"default": "Wakatime"
			},
			"minimumSize": {
				"width": 1,
				"height": 1
			},
			"maximumSize": {
				"width": 2000,
				"height": 2000
			},
			"preferredDockedSize": {
				"width": 330,
				"height": 400
			},
			"preferredFloatingSize": {
				"width": 300,
				"height": 400
			},
			"icons": [
				{
					"width": 32,
					"height": 32,
					"path": "icons/dark.png",
					"scale": [1, 2],
					"theme": ["dark", "darkest"],
					"species": ["generic"]
				},
				{
					"width": 32,
					"height": 32,
					"path": "icons/light.png",
					"scale": [1, 2],
					"theme": ["lightest", "light"],
					"species": ["generic"]
				}
			]
		}
	],
	"icons": [
		{
			"width": 48,
			"height": 48,
			"path": "icons/plugin-icon.png",
			"scale": [1, 2],
			"theme": ["all"],
			"species": ["pluginList"]
		}
	],
	"build": {
		"cep": {
			"enableInputEventBridge": true
		}
	}
}

